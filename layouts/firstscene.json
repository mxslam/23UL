      "type": "TextObject::Text",
      "underlined": false,
      "variables": [],
      "behaviors": [],
      "string": "           Itran               à         L'Idéal perdu\n\n         Des miliards pour un seul chagrin",
      "font": "SadanaSquare.ttf",
      "characterSize": 50,
      "color": {
        "b": 238,
